{
  "name": "Arena",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$className": "Folder",
        "data": {
          "$path": "../../shared/data"
        },
        "util": {
          "$path": "../../shared/util"
        },
        "RemoteEvents": {
          "$path": "../../shared/RemoteEvents.luau"
        },
        "PlacesConfig": {
          "$path": "../../shared/PlacesConfig.luau"
        },
        "config": {
          "$path": "src/shared/config"
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
    },
    "Lighting": {
      "$properties": {
        "Ambient": [0, 0, 0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
